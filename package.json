{
  "name": "Vimtur",
  "version": "4.1.0",
  "description": "Vimtur Media library and viewer.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/simplyboo6/Vimtur"
  },
  "dependencies": {
    "ajv": "^6.9.2",
    "args": "5.0.1",
    "better-ajv-errors": "^0.6.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "deepmerge": "^3.2.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "~2.1.0",
    "gm": "^1.23.0",
    "http-auth": "~3.2.3",
    "inflight": "^1.0.6",
    "mongodb": "^3.1.10",
    "once": "^1.4.0",
    "path-is-inside": "^1.0.2",
    "rimraf": "^2.6.2",
    "socket.io": "~2.1.1",
    "strip-json-comments": "^2.0.1",
    "walk": "^2.3.9",
    "wrappy": "^1.0.2"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build-config-schema": "typescript-json-schema ./tsconfig.json --strictNullChecks --required --noExtraProps Configuration.Main -o dist/config.schema.json",
    "build-media-schema": "typescript-json-schema ./tsconfig.json --strictNullChecks --required --noExtraProps --refs false BaseMedia -o dist/media.schema.json",
    "build-media-update-schema": "typescript-json-schema ./tsconfig.json --strictNullChecks --required --noExtraProps UpdateMedia -o dist/media-update.schema.json",
    "build-subset-schema": "typescript-json-schema ./tsconfig.json --strictNullChecks --required --noExtraProps SubsetConstraints -o dist/subset.schema.json",
    "build-ts": "tsc -p .",
    "build": "yarn build-ts && yarn build-config-schema && yarn build-media-schema && yarn build-media-update-schema && yarn build-subset-schema",
    "clean": "rm -rf dist/* *.tsbuildinfo",
    "lint": "./node_modules/@luvies/config/scripts/lint.sh",
    "fix": "./node_modules/@luvies/config/scripts/lint.sh fix",
    "lint-web": "eslint -c ./web/eslintrc.json web/js/src/*.js",
    "lint-web:fix": "eslint -c ./web/eslintrc.json web/js/src/*.js --fix"
  },
  "author": "SimplyBoo",
  "license": "MIT",
  "devDependencies": {
    "@luvies/config": "^1.5.0",
    "@types/args": "^3.0.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.17.0",
    "@types/fluent-ffmpeg": "^2.1.10",
    "@types/gm": "^1.18.3",
    "@types/mongodb": "^3.1.28",
    "@types/path-is-inside": "^1.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/socket.io": "2.1.2",
    "@types/strip-json-comments": "^0.0.30",
    "@types/walk": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.4.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "typescript-json-schema": "^0.38.3"
  }
}
